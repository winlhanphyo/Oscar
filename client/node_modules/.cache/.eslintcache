..preFormData });\n    const error = validation(false);\n    let preErrorForm = errorForm;\n    if (!error) {\n      preErrorForm[name] = error;\n      setErrorForm({\n        ...preErrorForm\n      });\n    }\n  };\n\n  const handleClick = (event) => {\n    event.preventDefault();\n    const validate = validation();\n    if (validate) {\n      setLoading(true);\n      let currentUrl = window.location.href;\n      let domain = currentUrl.split(\"/forget-password\")[0];\n      let param = formData;\n      param.domainUrl = domain;\n      axios.post('/forget-password', param).then((response) => {\n        console.log(response);\n        setLoading(false);\n        swal(\"Success\", \"Password Reset link is sent to your email successfully\", \"success\").then(() => {\n          let preFormData = formData;\n          preFormData.email = \"\";\n          setFormData({ ...preFormData });\n        });\n      }).catch((error) => {\n        setLoading(false);\n        swal(\"Oops!\", \"Email does not exists\", \"error\");\n        console.log(error);\n      });\n    } else {\n      setLoading(false);\n    }\n  }\n\n  return (\n    <>\n      <Header />\n      <Cart />\n\n      {loading && <LoadingSpinner text=\"Logging in...\" />}\n\n      {/* <!-- Title page --> */}\n      <section class=\"bg-img1 txt-center p-lr-15 p-tb-92\" style={{ backgroundImage: \"url('poto/a2.jpg')\" }}>\n        <h2 class=\"ltext-105 cl0 txt-center\">\n          Reset Your Password\n        </h2>\n      </section>\n\n      {/* <!-- Content page --> */}\n      <section class=\"bg0 p-t-104 p-b-116\">\n        <div class=\"container\">\n          <div class=\"flex-w flex-tr flex-c-m\">\n            <div class=\"size-210 bor10 p-lr-70 p-t-55 p-b-70 p-lr-15-lg w-full-md\">\n              <form>\n                <h6 class=\"stext-105 cl2 txt-center p-b-30\">\n                  We will send you an email to reset your password\n                </h6>\n\n                <div\n                  className={error